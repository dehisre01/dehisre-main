                    <elementRef key="lb" minOccurs="0"/>
                    <elementRef key="pb" minOccurs="0"/>
                    <elementRef key="subst" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="unclear" minOccurs="0" maxOccurs="unbound"/>
                </sequence>
            </content>
        </elementSpec>

        <elementSpec ident="item" mode="change">
            <desc>Ein Eintrag innerhalb einer Liste. Kann einfachen Text, Hervorhebungen, Entitäten
                und Inline-Anmerkungen enthalten.</desc>
            <content>
                <sequence preserveOrder="false">
                    <textNode/>
                    <elementRef key="add" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="choice" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="date" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="del" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="foreign" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="gap" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="hi" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="lb" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="list" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="measure" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="persName" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="placeName" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="subst" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="supplied" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="table" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="time" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="unclear" minOccurs="0" maxOccurs="unbound"/>
                </sequence>
            </content>
        </elementSpec>

        <elementSpec ident="cell" mode="change">
            <classes>
                <memberOf key="att.global.rendition"/>
            </classes>
            <content>
                <sequence preserveOrder="false">
                    <textNode/>
                    <elementRef key="add" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="choice" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="date" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="del" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="foreign" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="gap" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="hi" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="lb" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="list" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="measure" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="persName" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="placeName" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="subst" minOccurs="0" maxOccurs="unbound"/>
