                    <elementRef key="time" minOccurs="0" maxOccurs="unbound"/>
                    <elementRef key="unclear" minOccurs="0" maxOccurs="unbound"/>
                </sequence>
            </content>
        </elementSpec>

        <elementSpec ident="head" mode="change">
            <content>
                <textNode/>
                <elementRef key="add" minOccurs="0" maxOccurs="unbound"/>
                <elementRef key="choice" minOccurs="0" maxOccurs="unbound"/>
                <elementRef key="date" minOccurs="0" maxOccurs="unbound"/>
